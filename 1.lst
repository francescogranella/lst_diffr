4905   
4906  ax_co2('poly14fit','Total_CO2',t,n)    = macc_poly14fit_enerdata_CO2('Total_CO2', 'a', t, n);
4907  bx_co2('poly14fit','Total_CO2',t,n)    = macc_poly14fit_enerdata_CO2('Total_CO2', 'b', t, n);
4908   
4909   
4911   
4912   
4913  #  PBackstop curve -------------------------------------
4914  pbacktime(t)  =  pback*(1-gback)**(tperiod(t)-1);
4915  cost1(t,n)    =  pbacktime(t)*sigma(t,n)/expcost2/1000;
4916   
4917   
4918   
4919  # TRANSITION TO BACKSTOP -------------------------------
4920  * It is directly related to settings from conf phase.
4921  * Shape, slope and convergence time are all taken into account here
4922   
4923  ## logistic pbtransition
4924  scalar x0 ;
4925  x0 = 7 + ((23-7)/2)  ;
4926  alpha(t) = 1/(1+exp(-0.25*(tperiod(t)-x0)));
4927   
4928   
4929   
4930  # BACKSTOP MULTIPLIER ----------------------------------
4931   
4932  # NOTE .....................................................................
4933  # Following evaluations give answer to the question:
4934  # Which multiplier would make my full-abatement MACcurve (MIU=1) coincide
4935  # to the previously-evaluated pbackstop (in every time-step)?
4936  #...........................................................................
4937   
4938  # Mx = back_end / (a bau + b bau^4)  -->  MIU = 1
4939  MXpback(t,n)  =  pbacktime(t)
4940                  /  ((ax_co2('poly14fit','Total_CO2',t,n) * 1)  +  (bx_co2('poly14fit','Total_CO2',t,n) * power(1,4)))  ;
4941   
4942   
4943  # NOTE ........................................................................
4944  # Before transition the original multiplier applies,
4945  # then a smooth  transition to the pback-curve is performed,
4946  # by progressively reducing the distance (according to the shaping alpha-param)
4947  #...............................................................................
4948   
4949  MXstart(n)   =  MXkali('ssp2marker')  ;
4950  MXend(t,n)   =  MXpback(t,n)  ;
4951  MXdiff(t,n)  =  max( MXstart(n)  - MXend(t,n),0) ;
4952   
4953  * Final coefficient values:
4954  mx(t,n)      =  MXstart(n)  -  alpha(t) * MXdiff(t,n);
4955   
