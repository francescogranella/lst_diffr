7779   
7780   
7781      ## Handle-collect loop
7782      loop(clt$(cltsolve(clt) and handlecollect(h(clt))), # HandleCollect tests for the solution status:
7783                                                  # 1: solution available
7784                                                  # 0: otherwise
7785   
7786          execute_loadhandle CO2;  # Update GAMS database with solution for the current instance of CO2-model
7787   
7788          solrep(iter,clt,'solvestat') = CO2.solvestat;  # save solvestat  (1 is ok)
7789          solrep(iter,clt,'modelstat') = CO2.modelstat;  # save modelstat  (1 or 2 is optimal or locally opt)
7790          solrep(iter,clt,'feas')      =  ( (CO2.solvestat eq 1 or CO2.solvestat eq 4) and ((CO2.modelstat eq 1) or (CO2.modelstat eq 2) or (CO2.modelstat eq 7)));
7791          solrep(iter,clt,'opt')       =  ( (CO2.solvestat eq 1) and ((CO2.modelstat eq 1) or (CO2.modelstat eq 2)));
