2653  * Timecontrol definitions
2654  tfirst(t) = yes$(tperiod(t) eq smin(tt,tperiod(tt)));
2655  tlast(t) = yes$(tperiod(t) eq smax(tt,tperiod(tt)));
2656   
2657  tnofirst(t) = yes$(not tfirst(t));
2658  tnolast(t)  = yes$(not tlast(t));
2659   
2660  * Fixed period nodes
2661  set tfix(t)    'fixed period nodes';
2662  tfix(t) = no;
2663   
2664  * Create and load fix variable
2665  $macro loadfix(name,idx,type) \
2666  &type FIX&name&&idx; \
2667  execute_load '%gdxfix%.gdx',FIX&name=&name;
2668   
2669  * Fix variable in tfix
2670  * ( up to card(tfix) timestep )
2671  $macro tfixvar(name,idx) \
2672  loadfix(name,idx,variable) \
2673  &name.fx&&idx$tfix(t) = FIX&name.l&&idx; \
2674  &name.l&&idx$tfix(t) = FIX&name.l&&idx;
2675   
2676  * Fix Variable in tfix+1
2677  * ( up to card(tfix)-1 timestep )
2678  * ( up to card(tfix) timestep )
2679  $macro tfix1var(name,idx) \
2680  loadfix(name,idx,variable) \
2681  loop((t,tfix(tp1))$pre(t,tp1), \
2682  &name.fx&&idx = FIX&name.l&&idx; \
2683  &name.l&&idx = FIX&name.l&&idx; \
2684  );
2685   
2686  * load parameter in tfix
2687  $macro tfixpar(name,idx) \
2688  loadfix(name,idx,parameter) \
2689  &name&&idx$tfix(t) = FIX&name&&idx;
2690   
2691  * combinations of fixes and regions
2692  $macro map_nt (reg(n) and (not tfix(t)))
2693  $macro map_nt1 (reg(n) and (not tfix(tp1)) and pre(t,tp1))
2694  $macro map_t (not tfix(t))
