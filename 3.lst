4727   
4728  * starting value averaged over last 10 year ( to minimize risk of high fluctuations)
4729  eland0(n)  = CtoCO2 * sum(yearlu, q_emi_valid_oscar('co2lu',yearlu,n)$((yearlu.val ge 2005) and (yearlu.val lt 2015))) / 10  ;
4730   
4731  loop(t,
4732  * UNIFORM LOGIC
4733      eland_bau('uniform',t,n)  =  eland0(n)*(1-deland)**(tperiod(t)-1) ;
4734  * DIFFERENTIATED
4735      # if negative, it remains fixed to its value
4736      # otherwise smmoth decreasing at DICE2016 rate.
4737      eland_bau('differentiated',t,n)  =  min(eland0(n)*(1-deland)**(tperiod(t)-1), eland0(n) ) ;
4738  );
4739   
4740  * global values
4741  PARAMETER global_eland_bau(*,t);
4742    global_eland_bau('uniform',t)  = sum(n, eland_bau('uniform',t,n))    ;
4743    global_eland_bau('differentiated',t)  = sum(n, eland_bau('differentiated',t,n))   ;
4744   
4745  PARAMETER cumeland_bau(*,t,n), global_cumeland_bau(*,t);
4746  cumeland_bau('uniform','1',n)= 0;
4747  cumeland_bau('differentiated','1',n)= 0;
4748   
4749  loop((t,tp1)$pre(t,tp1),
4750   cumeland_bau('uniform', tp1, n) =  cumeland_bau('uniform', t, n) + eland_bau('uniform',t,n);
4751   cumeland_bau('differentiated', tp1, n) =  cumeland_bau('differentiated', t, n) + eland_bau('differentiated',t,n);
4752  );
4753   
4754  global_cumeland_bau('uniform',t) = sum(n, cumeland_bau('uniform',t,n)) ;
4755  global_cumeland_bau('differentiated',t) = sum(n, cumeland_bau('differentiated',t,n)) ;
