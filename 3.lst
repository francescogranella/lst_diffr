2619  * Timecontrol definitions
2620  tfirst(t) = yes$(tperiod(t) eq smin(tt,tperiod(tt)));
2621  tlast(t) = yes$(tperiod(t) eq smax(tt,tperiod(tt)));
2622   
2623  tnofirst(t) = yes$(not tfirst(t));
2624  tnolast(t)  = yes$(not tlast(t));
2625   
2626  * Fixed period nodes
2627  set tfix(t)    'fixed period nodes';
2628  tfix(t) = no;
2629   
2630  * Create and load fix variable
2631  $macro loadfix(name,idx,type) \
2632  &type FIX&name&&idx; \
2633  execute_load '%gdxfix%.gdx',FIX&name=&name;
2634   
2635  * Fix variable in tfix
2636  * ( up to card(tfix) timestep )
2637  $macro tfixvar(name,idx) \
2638  loadfix(name,idx,variable) \
2639  &name.fx&&idx$tfix(t) = FIX&name.l&&idx; \
2640  &name.l&&idx$tfix(t) = FIX&name.l&&idx;
2641   
2642  * Fix Variable in tfix+1
2643  * ( up to card(tfix)-1 timestep )
2644  * ( up to card(tfix) timestep )
2645  $macro tfix1var(name,idx) \
2646  loadfix(name,idx,variable) \
2647  loop((t,tfix(tp1))$pre(t,tp1), \
2648  &name.fx&&idx = FIX&name.l&&idx; \
2649  &name.l&&idx = FIX&name.l&&idx; \
2650  );
2651   
2652  * load parameter in tfix
2653  $macro tfixpar(name,idx) \
2654  loadfix(name,idx,parameter) \
2655  &name&&idx$tfix(t) = FIX&name&&idx;
2656   
2657  * combinations of fixes and regions
2658  $macro map_nt (reg(n) and (not tfix(t)))
2659  $macro map_nt1 (reg(n) and (not tfix(tp1)) and pre(t,tp1))
2660  $macro map_t (not tfix(t))
