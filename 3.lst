4497  ## CONF
4498  *-------------------------------------------------------------------
4499  * Definition of the global flags and settings specific to the module
4501   
4502  prodshare('labour', n) = 0.7; #original RICE/DICE value
4504  prodshare('capital', n) = 1 - prodshare('labour', n);
4505   
4506   
4507  * Starting GDP
4508  q0(n) = ykali('1',n);
4509   
4510   ##  BASELINE PER-CAPITA GROWTH ------------------------
4511  * Baseline per-capita growth
4512  loop((t,tp1)$(pre(t,tp1) and tnolast(t)),
4513  basegrowthcap(t,n) = ((( (ykali(tp1,n)/pop(tp1,n)) / (ykali(t,n)/pop(t,n)) )**(1/tstep)) - 1 );
4514  );
4515   
4516  ##  SAVINGS RATE --------
4517  * Optimal long-run Savings rate
4518  optlr_savings(n) = (dk + .004)/(dk + .004*elasmu + prstp)*prodshare('capital',n);
4519   
4520  * Evaluate converging Savings Rate
4521  * Linear interpolation: S0 + (Send - S0)*(t - t0)/(tend - t0)
4522  fixed_savings(t,n) = s0('savings_rate', '1', n) + (optlr_savings(n) - s0('savings_rate', '1', n)) * (tperiod(t) - 1)/(smax(tt,tperiod(tt)) - 1);
4523   
4524   
4525  ##  DYNAMIC CALIBRATION OF TFP FROM BASELINE AND SCENARIO ---------------------
4526  * set capital first value
4527  k_tfp('1',n)  =  k0('fg', '1', n);
4528   
4529  * retrieve tfp from reverting the Cobb-Douglas Production Function based on fixed investment rates iteratively
4530  loop((t,tp1)$pre(t,tp1),
4531     # Investments
4532     i_tfp(t,n)  =  fixed_savings(t,n)  * ykali(t,n)   ;
4533     # Capital
4534     k_tfp(tp1,n)  =  ((1-dk)**tstep) * k_tfp(t,n)  +  tstep * i_tfp(t,n)  ;
4535     # TFP of current scenario (explicited from Cobb-Douglas prod. function)
4536     tfp(t,n)  =  ykali(t,n) / {
4537                                   ( pop(t,n)/1000
4538                                   )**prodshare('labour',n) *
4539                                   k_tfp(t,n)**prodshare('capital',n) *
4541                                   1
4542                                };
4543  );
4544  tfp(t,n)$tlast(t) = sum(tt$pre(tt,t), tfp(tt,n));
